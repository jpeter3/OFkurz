(
    inlet
    {
        type wall;
        faces
        (
           
            (2 0 1 3)
        );
        
    }
    
    atmosphere
    {
        type wall;
        faces
        (
            
            (2 3 7 6)
            (3 1 5 7)
        );
    }
    ground
    {
        type wall;
        faces
        (
            
            (0 4 5 1)
        );
    }
    outlet
    {
        type wall;
        faces
        (
            (6 7 5 4)
        );
    }
        
    sym
    {
        type symmetry;
        faces
        (
            (2 6 4 0) 
        );
    }
      

);

